
<div class="side-bar-header">
  <div class="add-btn">
    <%= link_to new_room_path do %>
      <span class='material-icons'>add</span>
    <% end %>
  </div>
</div>

<div class="rooms">
  <% if user_signed_in? %>
    <% current_user.rooms.each do |room| %>
      <div class="room">
        <div class="room-name">
          <%= link_to room.name, room_memos_path(room) %>
        </div>
      </div>
    <% end %>
  <% end %>
</div>