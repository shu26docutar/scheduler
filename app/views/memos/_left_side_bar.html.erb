<div class="rooms">
    <div class="add-btn">
        <%= link_to new_room_path do %>
        <span class='material-icons'>add</span>
        <% end %>
    </div>

    <div class="room_area">
    <% @rooms.each do |room| %>
        <div class="rooms">
            <%= link_to room.name, rooms_path(room), id: "room-name"%>
        </div>
    <% end %>
</div>
</div> 