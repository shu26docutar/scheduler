<h1>Listing events</h1>

<% form_with() %>
  <div id="events_calendar">
  </div>

  <%= link_to 'top', root_path %>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Start time</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.title %></td>
          <td><%= event.start_time %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

