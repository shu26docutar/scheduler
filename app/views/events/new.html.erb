<%= form_with model: @event, local: true do |f| %>

    <div class="schedule_title">
        <%= f.text_field :title, class: 'form-event', placeholder: 'Title' %>
    </div>

    <div class="schedule_time">
        <%= f.text_field :first_time, class: 'form_time', placeholder: 'times' %>
    </div>

    <div class="submit">
        <%= f.submit '保存'%>
    </div>
<% end %> 
